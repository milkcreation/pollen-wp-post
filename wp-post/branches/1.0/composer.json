{
  "name": "pollen-solutions/wp-post",
  "description": "Pollen Wordpress Post Component.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-post/",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "post"
  ],
  "time": "2021-05-12",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/pagination": "^1.0",
    "pollen-solutions/support": "^1.0",
    "pollen-solutions/translation": "^1.0",
    "pollen-solutions/wp-term": "^1.0",
    "pollen-solutions/wp-user": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpPost\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpPost\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
